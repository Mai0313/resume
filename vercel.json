{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "Personal Website",
  "framework": "vite",
  "devCommand": "yarn dev",
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
